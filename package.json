{
  "name": "@danmartens/immutable",
  "repository": {
    "url": "git+https://github.com/danmartens/immutable.git"
  },
  "version": "0.3.1",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "types:check": "tsc --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint .",
    "build": "rm -rf dist && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript && tsc -p tsconfig.build.json",
    "publish": "npm publish --access public --provenance",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.3",
    "babel-jest": "^29.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-perfectionist": "^4.15.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rollup": "^4.54.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "turbo": "^2.7.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "dependenciesMeta": {
    "turbo": {
      "built": true
    }
  },
  "packageManager": "yarn@4.12.0"
}
